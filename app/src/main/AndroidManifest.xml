<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.poliodrop">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.PolioDrop">
        <activity
            android:name=".WorkerAndUserList"
            android:exported="false" />
        <activity
            android:name=".RequestsActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ActivitySignup"
            android:screenOrientation="portrait" />
        <activity
            android:name=".UserDashboard"
            android:screenOrientation="portrait" />
        <activity
            android:name=".WorkerDashboard"
            android:screenOrientation="portrait" />
        <activity
            android:name=".AdminDashboard"
            android:screenOrientation="portrait" />
        <activity
            android:name=".RequestList"
            android:screenOrientation="portrait" />
        <activity
            android:name=".RequestDetail"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ContactActivity"
            android:screenOrientation="portrait" />
    </application>

</manifest>